jQuery(document).ready(function($){let share_container=$('.addtoany_share_save_container'),share_toggle=$('#share-post');share_container.appendTo(share_toggle);$(document).on('click',function(e){let target=$(e.target);if(target.is(share_toggle)){if(target.hasClass('open')){target.removeClass('open');share_container.slideUp();}else{target.addClass('open');share_container.slideDown();}}else{share_toggle.removeClass('open');share_container.slideUp();}});});